rorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/support/web-development/online-store/src/index.js",[],"/Users/support/web-development/online-store/src/App.js",["95"],"/Users/support/web-development/online-store/src/components/Navbar.js",["96","97","98","99"],"import React, { useState, useEffect, useContext } from 'react';\nimport { CognitoContext } from '../context/Cognito'\nimport { useHistory } from \"react-router-dom\";\nimport './Navbar.css'\nimport axios from 'axios'\n\nconst Navbar = () => {\n    const [search, setSearch] = useState(false)\n    const [searchList, setSearchList] = useState(\"\")\n    const [searchItem, setSearchItem] = useState(false)\n    const [searchKey, setSearchKey] = useState(\"\")\n    const [searchComplete, setSearchComplete] = useState([])\n    const { getSession, logout, user } = useContext(CognitoContext)\n    let history = useHistory();\n\n    const toggleSearchIcon = () => {\n        if (!search) {\n            setSearch(!search)\n            if (!searchList) {\n                axios.get('https://ac7j0yqyw7.execute-api.us-east-2.amazonaws.com/dev/products')\n                    .then((response) => {\n                        setSearchList(response.data)\n                    });\n            }\n        } else {\n            setSearch(!search)\n            setSearchItem(false)\n        }\n    }\n    useEffect(() => {\n        if (searchKey.length > 3) {\n            setSearchItem(true)\n            let val = searchKey.toLowerCase();\n            let arr = []\n            for (let i = 0; i < searchList.length; i++) {\n                if (searchList[i].name.toLowerCase().includes(val) || searchList[i].brand.toLowerCase().includes(val)) {\n                    arr.push(searchList[i])\n                }\n            }\n            arr.splice(3, arr.length)\n            setSearchComplete(arr)\n        } else {\n            setSearchItem(false)\n        }\n\n    }, [searchKey])\n\n    useEffect(() => {\n        getSession()\n    }, [])\n\n    const result = searchComplete.map((item) =>\n        <div onClick={()=> history.push(`/item/${item.id}`)} className=\"search--item__active\">\n            <img className=\"search--item__image\" src={item.image} />\n            <div className=\"search--item__box\">\n                <div className=\"search--item__name\">{item.name}</div>\n                <div className=\"search--item__brand\">{item.brand}</div>\n                <div className=\"search--item__price\">${item.price}</div>\n            </div>\n        </div>\n    );\n\n    return (\n        <div className=\"nav--container\">\n            <img onClick={() => history.push(\"/\")} className=\"nav--logo\" src=\"/images/Logo.png\" alt=\"logo cloth\" />\n            <div className=\"nav--button__container\">\n                <button onClick={() => history.push(\"/product\")} className=\"nav--button\"> Men </button>\n                <button onClick={() => history.push(\"/product\")} className=\"nav--button\"> Women </button>\n                <button onClick={() => history.push(\"/product\")} className=\"nav--button\"> Accessories </button>\n                <button onClick={() => console.log(localStorage)} className=\"nav--button\"> Kids </button>\n                <button onClick={() => localStorage.clear()} className=\"nav--button\"> Offers </button>\n            </div>\n            <div className=\"nav--icon__container\">\n                <i onClick={() => toggleSearchIcon()} className='bx bx-search-alt-2 nav--icons'></i>\n                <div className=\"nav--search__container\">\n                    <input spellCheck=\"false\" value={searchKey} onChange={(e) => setSearchKey(e.target.value)} className={`nav--search${search ? \"\" : \"__inactive\"}`} autoComplete=\"off\" placeholder=\"Search...\" required />\n                    <div className={`nav--search__item${searchItem ? \"\" : \"__inactive\"}`}>\n                        {result}\n                    </div>\n                </div>\n                <i onClick={() => history.push(\"/checkout\")} className='bx bxs-cart-alt nav--icons'></i>\n                {user ? \n                <i onClick={() => logout().then(history.push(\"/signin\"))} className='bx bx-log-in nav--icons'></i>\n                :\n                <i onClick={() => history.push(\"/signin\")} className='bx bxs-user nav--icons'></i>\n                }\n            </div>\n\n        </div>\n    )\n}\n\nexport default Navbar",["100","101"],"/Users/support/web-development/online-store/src/components/Slides.js",[],"/Users/support/web-development/online-store/src/components/Items.js",[],"/Users/support/web-development/online-store/src/components/Footer.js",[],"/Users/support/web-development/online-store/src/pages/Main.js",[],"/Users/support/web-development/online-store/src/pages/Product.js",[],"/Users/support/web-development/online-store/src/components/Filter.js",["102"],"import React, { useState, useEffect } from 'react'\nimport './Filter.css'\n\nconst categoriesList = [\"T-Shirt\", \"Shorts\", \"Jeans\", \"Sweater\", \"Shirts\"]\nconst brandList = [\"Adidas\", \"Nike\", \"Uniclo\", \"Lacoste\", \"Under Armor\", \"No brand\"]\nconst priceList = [{\n    name: \"under $50\",\n    price: [0, 50]\n}, {\n    name: \"$51 ~ $100\",\n    price: [51, 100]\n},\n{\n    name: \"over $100\",\n    price: [101, 5000]\n}]\n\nconst Filter = ({ setProducts, productList }) => {\n    const [category, setCategory] = useState(\"\")\n    const [price, setPrice] = useState(\"\")\n    const [brand, setBrand] = useState(\"\")\n\n    useEffect(() => {\n        let updateCart = productList\n        let categoryCart = []\n        if (!category) {\n            categoryCart = updateCart\n        } else {\n            for (let i = 0; i < updateCart.length; i++) {\n                if (updateCart[i].tag === category) {\n                    categoryCart.push(updateCart[i])\n                }\n            }\n        }\n        let brandCart = []\n        if (!brand) {\n            brandCart = categoryCart\n        } else {\n            for (let i = 0; i < categoryCart.length; i++) {\n                if (categoryCart[i].brand === brand) {\n                    brandCart.push(categoryCart[i])\n                }\n            }\n        }\n        let priceCart = []\n        if (!price) {\n            priceCart = brandCart\n        } else {\n            for (let i = 0; i < brandCart.length; i++) {\n                if (brandCart[i].price > price[0] && brandCart[i].price < price[1]) {\n                    priceCart.push(brandCart[i])\n                }\n            }\n        }\n        setProducts(priceCart)\n    }, [category, price, brand])\n\n    const handleCategory = (categoryItem) => {\n        if (categoryItem === category) {\n            setCategory(\"\")\n        } else {\n            setCategory(categoryItem)\n        }\n    }\n\n    const handleBrand = (brandItem) => {\n        if (brandItem === brand) {\n            setBrand(\"\")\n        } else {\n            setBrand(brandItem)\n        }\n    }\n\n    const handlePrice = (priceItem) => {\n        if (priceItem === price) {\n            setPrice(\"\")\n        } else {\n            setPrice(priceItem)\n        }\n    }\n\n    return (\n        <div className=\"filter--container\">\n            <div className=\"filter--title__main\">\n                Filters\n            </div>\n            <div className=\"filter--list__container\">\n                <div className=\"filter--title\">\n                    Categories\n                </div>\n                {categoriesList.map((item, index) =>\n                    <div key={index} className=\"filter--list\">\n                        <div onClick={() => handleCategory(item)} className={item === category ? \"filter--checkbox__selected\" : \"filter--checkbox\"} />\n                        <label> {item} </label>\n                    </div>\n                )}\n            </div>\n            <div className=\"filter--list__container\">\n                <div className=\"filter--title\">\n                    Brands\n                </div>\n                {brandList.map((item, index) =>\n                    <div key={index} className=\"filter--list\">\n                        <div onClick={() => handleBrand(item)} className={item === brand ? \"filter--checkbox__selected\" : \"filter--checkbox\"} />\n                        <label> {item} </label>\n                    </div>\n                )}\n            </div>\n            <div className=\"filter--list__container\">\n                <div className=\"filter--title\">\n                    Prices\n                </div>\n                {priceList.map((item, index) =>\n                    <div key={index} className=\"filter--list\">\n                        <div onClick={() => handlePrice(item.price)} className={price === item.price ? \"filter--checkbox__selected\" : \"filter--checkbox\"}>\n                        </div>\n                        <label> {item.name} </label>\n                    </div>\n                )}\n\n            </div>\n        </div>\n    )\n}\n\nexport default Filter","/Users/support/web-development/online-store/src/components/Products.js",[],"/Users/support/web-development/online-store/src/pages/Item.js",["103"],"import React, { useEffect, useState } from 'react';\nimport Navbar from '../components/Navbar'\nimport Footer from '../components/Footer'\nimport ItemDetail from '../components/ItemDetail'\nimport { useParams } from \"react-router-dom\";\nimport axios from 'axios'\nimport { useHistory } from \"react-router-dom\";\n\n\nconst Item = () => {\n    let history = useHistory();\n    const [item, setItem] = useState({})\n    const [loading, setLoading] = useState(true)\n    let { id } = useParams();\n    const url = `https://ac7j0yqyw7.execute-api.us-east-2.amazonaws.com/dev/products/${id}`\n\n    useEffect(() => {\n        axios.get(url)\n            .then((response) => {\n                if (respo