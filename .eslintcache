rorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/support/web-development/online-store/src/index.js",[],"/Users/support/web-development/online-store/src/App.js",["94"],"/Users/support/web-development/online-store/src/components/Navbar.js",["95","96","97"],"/Users/support/web-development/online-store/src/components/Slides.js",[],["98","99"],"/Users/support/web-development/online-store/src/components/Items.js",[],"/Users/support/web-development/online-store/src/components/Footer.js",[],"/Users/support/web-development/online-store/src/pages/Main.js",[],"/Users/support/web-development/online-store/src/pages/Product.js",[],"/Users/support/web-development/online-store/src/components/Filter.js",["100"],"import React, { useState, useEffect } from 'react'\nimport './Filter.css'\n\nconst categoriesList = [\"T-Shirt\", \"Shorts\", \"Jeans\", \"Sweater\", \"Shirts\"]\nconst brandList = [\"Adidas\", \"Nike\", \"Uniclo\", \"Lacoste\", \"Under Armor\", \"No brand\"]\nconst priceList = [{\n    name: \"under $50\",\n    price: [0, 50]\n}, {\n    name: \"$51 ~ $100\",\n    price: [51, 100]\n},\n{\n    name: \"over $100\",\n    price: [101, 5000]\n}]\n\nconst Filter = ({ setProducts, productList }) => {\n    const [category, setCategory] = useState(\"\")\n    const [price, setPrice] = useState(\"\")\n    const [brand, setBrand] = useState(\"\")\n\n    useEffect(() => {\n        let updateCart = productList\n        let categoryCart = []\n        if (!category) {\n            categoryCart = updateCart\n        } else {\n            for (let i = 0; i < updateCart.length; i++) {\n                if (updateCart[i].tag === category) {\n                    categoryCart.push(updateCart[i])\n                }\n            }\n        }\n        let brandCart = []\n        if (!brand) {\n            brandCart = categoryCart\n        } else {\n            for (let i = 0; i < categoryCart.length; i++) {\n                if (categoryCart[i].brand === brand) {\n                    brandCart.push(categoryCart[i])\n                }\n            }\n        }\n        let priceCart = []\n        if (!price) {\n            priceCart = brandCart\n        } else {\n            for (let i = 0; i < brandCart.length; i++) {\n                if (brandCart[i].price > price[0] && brandCart[i].price < price[1]) {\n                    priceCart.push(brandCart[i])\n                }\n            }\n        }\n        setProducts(priceCart)\n    }, [category, price, brand])\n\n    const handleCategory = (categoryItem) => {\n        if (categoryItem === category) {\n            setCategory(\"\")\n        } else {\n            setCategory(categoryItem)\n        }\n    }\n\n    const handleBrand = (brandItem) => {\n        if (brandItem === brand) {\n            setBrand(\"\")\n        } else {\n            setBrand(brandItem)\n        }\n    }\n\n    const handlePrice = (priceItem) => {\n        if (priceItem === price) {\n            setPrice(\"\")\n        } else {\n            setPrice(priceItem)\n        }\n    }\n\n    return (\n        <div className=\"filter--container\">\n            <div className=\"filter--title__main\">\n                Filters\n            </div>\n            <div className=\"filter--list__container\">\n                <div className=\"filter--title\">\n                    Categories\n                </div>\n                {categoriesList.map((item, index) =>\n                    <div key={index} className=\"filter--list\">\n                        <div onClick={() => handleCategory(item)} className={item === category ? \"filter--checkbox__selected\" : \"filter--checkbox\"} />\n                        <label> {item} </label>\n                    </div>\n                )}\n            </div>\n            <div className=\"filter--list__container\">\n                <div className=\"filter--title\">\n                    Brands\n                </div>\n                {brandList.map((item, index) =>\n                    <div key={index} className=\"filter--list\">\n                        <div onClick={() => handleBrand(item)} className={item === brand ? \"filter--checkbox__selected\" : \"filter--checkbox\"} />\n                        <label> {item} </label>\n                    </div>\n                )}\n            </div>\n            <div className=\"filter--list__container\">\n                <div className=\"filter--title\">\n                    Prices\n                </div>\n                {priceList.map((item, index) =>\n                    <div key={index} className=\"filter--list\">\n                        <div onClick={() => handlePrice(item.price)} className={price === item.price ? \"filter--checkbox__selected\" : \"filter--checkbox\"}>\n                        </div>\n                        <label> {item.name} </label>\n                    </div>\n                )}\n\n            </div>\n        </div>\n    )\n}\n\nexport default Filter","/Users/support/web-development/online-store/src/components/Products.js",[],"/Users/support/web-development/online-store/src/pages/Item.js",["101"],"import React, { useEffect, useState } from 'react';\nimport Navbar from '../components/Navbar'\nimport Footer from '../components/Footer'\nimport ItemDetail from '../components/ItemDetail'\nimport { useParams } from \"react-router-dom\";\nimport axios from 'axios'\nimport { useHistory } from \"react-router-dom\";\n\n\nconst Item = () => {\n    let history = useHistory();\n    const [item, setItem] = useState({})\n    const [loading, setLoading] = useState(true)\n    let { id } = useParams();\n    const url = `https://ac7j0yqyw7.execute-api.us-east-2.amazonaws.com/dev/products/${id}`\n\n    useEffect(() => {\n        axios.get(url)\n            .then((response) => {\n                if (response.data.errorMessage) {\n                    history.push(\"/product\")\n                } else {\n                    setItem(response.data)\n                    setLoading(false)\n                }\n            });\n    }, [url]);\n\n\n    return (\n        <div>\n            <Navbar />\n            <ItemDetail item={item} loading={loading}/>\n            <Footer />\n        </div>\n    )\n}\n\nexport default Item","/Users/support/web-development/online-store/src/components/ItemDetail.js",[],"/Users/support/web-development/online-store/src/pages/Signup.js",[],"/Users/support/web-development/online-store/src/pages/Signin.js",[],"/Users/support/web-development/online-store/src/components/SignupForm.js",[],"/Users/support/web-development/online-store/src/components/SigninForm.js",[],"/Users/support/web-development/online-store/src/pages/CheckoutPage.js",[],"/Users/support/web-development/online-store/src/components/Checkout.js",["102"],"/Users/support/web-development/online-store/src/components/Address.js",["103"],"import react from 'react'\n\nconst Address = ({ setUserInfo }) => {\n    return (<div>\n        <input/>\n        <br/>\n        <input/>\n        <br/>\n        <input/>\n        <br/>\n        <input/>\n        <br/>\n        <input/>\n        <br/>\n        <input/>\n        <br/>\n        <button onClick={()=> setUserInfo(false)}>\n            back\n        </button>\n    </div>)\n}\n\nexport default Address","/Users/support/web-development/online-store/src/UserPool.js",[],"/Users/support/web-development/online-store/src/context/Cognito.js",[],"/Users/support/web-development/online-store/src/helper/Route.js",[],{"ruleId":"104","severity":1,"message":"105","line":18,"column":6,"nodeType":"106","endLine":18,"endColumn":8,"suggestions":"107"},{"ruleId":"104","severity":1,"message":"108","line":47,"column":8,"nodeType":"106","endLine":47,"endColumn":19,"suggestions":"109"},{"ruleId":"104","severity":1,"message":"105","line":51,"column":8,"nodeType":"106","endLine":51,"endColumn":10,"suggestions":"110"},{"ruleId":"111","severity":1,"message":"112","line":55,"column":13,"nodeType":"113","endLine":55,"endColumn":69},{"ruleId":"114","replacedBy":"